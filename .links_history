map (fun (x) {~nil})([1,2,3]) ;;
var nil = [] ;;
fun l(x) {x(~x)} ;;
fun k(x) {x(x)} ;;
sig j : (forall a. a) -> (forall a. a)   fun j (x) {~x(x)} ;;
sig i : (forall a. a) -> (forall a. a)   fun i (x) {x(~x)} ;;
sig h : (forall a. a) -> (forall a. a)   fun h (x) {~x} ;;
sig g : (forall a. a) -> (forall a. a)   fun g (x) {x} ;;
fun f(x) {x} ;;
fun (x) {x} ;;